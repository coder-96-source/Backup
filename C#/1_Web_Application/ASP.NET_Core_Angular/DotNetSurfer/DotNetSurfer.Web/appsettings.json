{
  "ConnectionStrings": {
    "DefaultConnection": "UseYourConnectionString"
  },

  "Blob": {
    "AccountName": "UseYourAccountName",
    "AccountKey": "UseYourAccountKey"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "Jwt": {
    "Key": "UseYourSecretKey",
    "Issuer": "https://localhost:5001/",
    "ExpireMinutes": "30"
  },

  "Session": {
    "ExpireMinutes": "30"
  },

  "AllowedHosts": "*"
}
